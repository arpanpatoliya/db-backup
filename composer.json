{
    "name": "arpanpatoliya/db-backup",
    "description": "A Laravel package for database backup with Google Drive integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arpan Patoliya",
            "email": "info.arpan08@gmail.com",
            "role": "Developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "Arpanpatoliya\\DBBackup\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^8.35"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arpanpatoliya\\DBBackup\\DBBackupServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
